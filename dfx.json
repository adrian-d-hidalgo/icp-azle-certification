{
  "canisters": {
    "users": {
      "type": "custom",
      "main": "backend/domain/users/main.ts",
      "candid": "backend/domain/users/main.did",
      "build": "npx azle users",
      "wasm": ".azle/users/users.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/users",
        "node_compatibility": true
      }
    },
    "patient-profiles": {
      "type": "custom",
      "main": "backend/domain/patient-profiles/main.ts",
      "candid": "backend/domain/patient-profiles/main.did",
      "build": "npx azle patient-profiles",
      "wasm": ".azle/patient-profiles/patient-profiles.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/patient-profiles",
        "node_compatibility": true
      }
    },
    "prescriptions": {
      "type": "custom",
      "main": "backend/domain/prescriptions/main.ts",
      "candid": "backend/domain/prescriptions/main.did",
      "build": "npx azle prescriptions",
      "wasm": ".azle/prescriptions/prescriptions.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/prescriptions",
        "node_compatibility": true
      }
    },
    "patient-app": {
      "type": "custom",
      "main": "backend/applications/patient-app/main.ts",
      "candid": "backend/applications/patient-app/main.did",
      "build": "npx azle patient-app",
      "wasm": ".azle/patient-app/patient-app.wasm",
      "gzip": true,
      "declarations": {
        "output": "test/dfx_generated/patient-app",
        "node_compatibility": true
      }
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
